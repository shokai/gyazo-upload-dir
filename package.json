  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "standard": "^8.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shokai/gyazo-upload-dir.git"
  },
  "bugs": {
    "url": "https://github.com/shokai/gyazo-upload-dir/issues"
