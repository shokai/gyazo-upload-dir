{
  "name": "gyazo-upload-dir",
  "version": "0.0.2",
  "description": "watch directory and upload images to gyazo",
  "main": "upload-gyazo.js",
  "bin": {
    "gyazo-upload-dir": "bin/cli.js"
  },
  "scripts": {
    "build": "babel src/ --out-dir lib/ --source-maps inline",
